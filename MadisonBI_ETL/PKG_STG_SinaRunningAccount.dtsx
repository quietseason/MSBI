        static void cleanFiles(FileSystemInfo dir, int rDays)
        {
            if (!dir.Exists) return;
            DirectoryInfo folder = dir as DirectoryInfo;
            FileSystemInfo[] files = folder.GetFileSystemInfos();
            for (int i = 0; i < files.Length; i++)
            {
                FileInfo file = files[i] as FileInfo;
                if (file != null)
                {
                    //It is a file
                    if (file.LastWriteTime < DateTime.Now.AddDays(-1 * rDays))
                    {
                        file.Delete();
                    }
                }
                else
                {
                    //It is a directory
                    cleanFiles(files[i], rDays);
                }
            }
        }
